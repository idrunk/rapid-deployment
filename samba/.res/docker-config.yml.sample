auth:
  - user: foo
    group: foo
    uid: 1000
    gid: 1000
    password: foo

global:
  - "force user = foo"
  - "force group = foo"

share:
  - name: share
    path: /share
    browsable: yes
    readonly: no
    guestok: no
    validusers: foo
    writelist: foo
    veto: no
    hidefiles: /_*/
    recycle: yes